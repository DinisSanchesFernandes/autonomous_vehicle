<launch>

    <!-- Load joint controller configurations from YAML file to parameter server -->
    <rosparam command="load" file="$(find autonomous_vehicle_control)/config/autonomous_vehicle_control.yaml"/>
  
    <!-- load the controllers -->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
      output="screen" ns="/autonomous_vehicle" 
      args="joint_controller left_axle_controller right_axle_controller right_motor_controller left_motor_controller"/>
      
  </launch>
